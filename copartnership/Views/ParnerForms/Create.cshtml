@model copartnership.Models.ParnerForm

@{
    ViewData["Title"] = "Create";
}



<hr />
<div class="IndexDiv">
    <div class="row">
    <div class="col-md-5">
        <form asp-action="Create" class="form-create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NationalId" class="control-label"></label>
                <input asp-for="NationalId" class="form-control" />
                <span asp-validation-for="NationalId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PartnerType" class="control-label"></label>
                <input asp-for="PartnerType" class="form-control" />
                <span asp-validation-for="PartnerType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Dept" class="control-label"></label>
                <input asp-for="Dept"  class="form-control" />
                <span asp-validation-for="Dept" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PartnerDate" class="control-label"></label>
                <input asp-for="PartnerDate" class="form-control" />
                <span asp-validation-for="PartnerDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ParnerDuration" class="control-label"></label>
                <input asp-for="ParnerDuration" class="form-control" />
                <span asp-validation-for="ParnerDuration" class="text-danger"></span>
            </div>



            <div class="form-group">
                <input type="submit" value="إنشاء" class="btn-1" />
                <a asp-action="Index" asp-controller="Home" class="btn-1">عودة</a>
            </div>
        </form>
    </div>
</div>
</div>

<div>
